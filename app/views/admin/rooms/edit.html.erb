<% title "Edit Room" %>
<% stylesheet "style_rooms" %>
<% javascript 'room_form' %>

<div id="wrapper" class="room-edit">
  <%= render 'form' %>
</div>


<script>
  $(function(){
    var roomFormView = new App.Rooms.FormView({ el: $("#room-form") });
    roomFormView.updateCellsCollection( <%= @room.to_hash.to_json.html_safe %> );
  });
</script>